module libexslt [system] [extern_c] {
    // Add "-Xcc -I$(SDKROOT)/usr/include/libxml2 -Xcc -I$(SDKROOT)/usr/include/libexslt" to OTHER_SWIFT_FLAGS in Xcode project.

    module exslt {
        header "exslt.h"
        export *
    }

    module exsltconfig {
        header "exsltconfig.h"
        export *
    }

    module exsltexports {
        header "exsltexports.h"
        export *
    }
}

module tidy [system] [extern_c] {
    // Add "-Xcc -I$(SDKROOT)/usr/include/tidy" to OTHER_SWIFT_FLAGS in Xcode project.

    module buffio {
        header "buffio.h"
        export *
    }

    module platform {
        header "platform.h"
        export *
    }

    module tidy {
        header "tidy.h"
        export *
    }

    module tidyenum {
        header "tidyenum.h"
        export *
    }
}
